{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [ "./src/*" ],
      "~/icons/*": [ "./node_modules/@lucide/astro/src/icons/*" ]
    }
  }
}
